\par\pard\plain \tx0\li346\fi14\sb60\sl264\slmult1\ltrch\loch {\f3\fs22\b1\i0\cf8 - Main Characters:}{\f2\fs22\b0\i0\cf1 <$Scr_Cs::1> Sarah, Evan<!$Scr_Cs::1>}
\par\plain {\f3\fs22\b1\i0\cf8 - Secondary Characters:}{\f2\fs22\b0\i0\cf1 <$Scr_Cs::1> Guido, Patti, Roxanne, Evan\loch\af2\hich\af2\dbch\af2\uc1\u8217\'92s softball coach, and several others in Evan's circle of friends<!$Scr_Cs::1><!$Scr_Ps::4>}
