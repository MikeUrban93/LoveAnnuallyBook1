\par\pard\plain \tx0\li346\fi14\sb60\sl264\slmult1\ltrch\loch {\f2\fs24\b1\i0\cf7 Patti: }{\f0\fs24\b0\i0\cf5 <$Scr_Cs::1>Upset and conflicted<!$Scr_Cs::1>}
\par\plain {\f2\fs24\b1\i0\cf7 Guido: }{\f0\fs24\b0\i0\cf5 <$Scr_Cs::1>Surprisingly compassionate<!$Scr_Cs::1><!$Scr_Ps::4><$Scr_Ps::2>}
